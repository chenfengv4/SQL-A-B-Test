CREATE TABLE assignments1 (
user_id int,
feature_flag_evaluation varchar(255),
evaluated_at date,
experiment_name varchar(255)
);
CREATE TABLE users1 (
user_id int,
city_name varchar(255),
signup_marketing_channel varchar(255),
first_order_at date
);
CREATE TABLE order1 (
  user_id int,
  order_type varchar(255),
  order_id int,
  order_value int,
  order_placed_at date
);
